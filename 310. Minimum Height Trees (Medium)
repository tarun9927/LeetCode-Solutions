class Solution:
    def findMinHeightTrees(self, n: int, edges: List[List[int]]) -> List[int]:
        graph={i:[] for i in range(n)}
        indegree={i:0 for i in range(n)}
        for edge in edges:
            graph[edge[0]].append(edge[1])
            graph[edge[1]].append(edge[0])
            indegree[edge[0]]+=1
            indegree[edge[1]]+=1
        queue,ans=[],[]
        vis={}
        for i in indegree:
            if indegree[i]==0:
                ans.append(i)
            if indegree[i]==1:
                queue.append(i)
                indegree[i]-=1
        while queue:
            temp=set()
            temp2=[]
            for u in queue:
                if u in vis: continue
                vis[u]=True
                temp2.append(u)
            queue=temp2
            for u in queue:
                for v in graph[u]:
                    if v not in vis:
                        indegree[v]-=1
                        if indegree[v]==1:
                            temp.add(v)
            if not temp:break
            queue=list(temp)
        return queue+ans
